        <!-- Weekly / Monthly stats -->
        <div class="row">
           
            <div class="col-lg-4">
                <!--user info table start-->
                <section class="panel">
                    <div class="panel-body">
                        <div class="task-thumb-details">
                            <h1><a href="#">Vishnu Enterprises</a></h1>
                            <p>Weekly Stats</p>
                        </div>
                    </div>
                    <table class="table table-hover personal-task">
                        <tbody>
                            <tr>
                                <td>Avgerage Sales</td>
                                <td id="average_sales"> 02</td>
                            </tr>
                            <tr>
                                <td>Total Dues</td>
                                <td id="total_dues"> 14</td>
                            </tr>
                            <tr>
                                <td>Total Advance</td>
                                <td id="total_advance"> 45</td>
                            </tr>
                            <tr>
                                <td>Total Customers</td>
                                <td id="total_customers"> 09</td>
                            </tr>
                        </tbody>
                    </table>
                </section>
                <!--user info table end-->
            </div>

            <!-- Graphs -->
            <div class="col-lg-8">
                <div id="container1">
                </div>
            </div>
        </div>
        <!-- Weekly / Monthly stats ends -->

<script src="~/Scripts/Sales_Analysis.js"></script>
<script src="~/Scripts/highcharts-3d.js"></script>
<script src="~/Scripts/highcharts.src.js"></script>
<script src="~/Scripts/highcharts.js"></script>

<script>
    $('#container1').highcharts({
        type: 'column',
        title: {
            text: 'SALES PER DAY'
        },
        xAxis: {

            categories: [1, 2, 3, 4, 5, 6]
        },

        yAxis: {
            title: {
                text: 'INR'
            },
            lineWidth: 2,
            lineColor: '#00aeef'

        },

        series: [{
            type: 'column',
            name: 'Sales',
            data: [25, 36, 59, 24, 65, 24],
            color: '#00aeef'
        }]
    });
</script>



